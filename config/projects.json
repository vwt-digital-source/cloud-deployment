{
    "projects": [
        {
            "projectId": "my-gcp-project",
            "triggers": [
                {
                    "triggerTemplate": {
                        "repoName": "github_my-github_my-gcp-project-repo",
                        "branchName": "master"
                    },
                    "filename": "cloudbuild.yaml"
                },
                {
                    "triggerTemplate": {
                        "repoName": "github_otherguy_some-3rd-party-repo",
                        "branchName": "master"
                    },
                    "runTrigger": {
                        "repoName": "github_my-github_my-gcp-project-repo",
                        "branchName": "master"
                    }
                }
            ],
            "firewallRules": [
                {
                    "name": "my-firewall-rule",
                    "direction": "INGRESS/EGRESS",
                    "action": "allow/deny",
                    "protocolRules": [
                        "protocol:port"
                    ],
                    "description": "my-description",
                    "network": "my-network",
                    "priority": 1000,
                    "sourceRanges": [
                        "0.0.0.0/0"
                    ]
                }
            ],
            "services": [
                "cloudbuild.googleapis.com",
                "pubsub.googleapis.com",
                "cloudfunctions.googleapis.com"
            ],
            "keyrings": [
                {
                    "name": "my-keyring",
                    "region": "europe-west1",
                    "keys": [
                        {
                            "name": "my-key",
                            "purpose": "ENCRYPT_DECRYPT"
                        }
                    ]
                }
            ],
            "odrlPolicy": {
                "uid": "my-first-gcp-project-policy",
                "permission": [
                    {
                        "target": "my-keyring",
                        "assignee": "user:me@myself.com",
                        "action": "roles/cloudkms.cryptoKeyEncrypter"
                    },
                    {
                        "target": "my-keyring",
                        "assignee": "serviceAccount:my-first-gcp-project@appspot.gserviceaccount.com",
                        "action": "roles/cloudkms.cryptoKeyDecrypter"
                    },
                    {
                        "target": "my-first-gcp-project",
                        "assignee": "user:me@myself.com",
                        "action": "roles/owner"
                    }
                ]
            }
        },
        {
            "projectId": "my-second-gcp-project",
            "triggers": [
                {
                    "triggerTemplate": {
                        "repoName": "github_my-github_my-gcp-project-repo",
                        "branchName": "master"
                    },
                    "filename": "cloudbuild.yaml"
                }
            ],
            "services": [
                "cloudbuild.googleapis.com",
                "appengine.googleapis.com"
            ],
            "appEngineRegion": "europe-west",
            "odrlPolicy": {
                "uid": "my-second-gcp-project-policy",
                "permission": [
                    {
                        "target": "my-second-gcp-project",
                        "assignee": "user:me@myself.com",
                        "action": "roles/owner"
                    }
                ]
            }
        },
        {
            "projectId": "my-third-gcp-project",
            "triggers": [
                {
                    "triggerTemplate": {
                        "repoName": "github_my-github_my-gcp-project-repo",
                        "branchName": "master"
                    },
                    "filename": "cloudbuild.yaml"
                }
            ],
            "odrlPolicy": {
                "uid": "my-third-gcp-project-policy",
                "permission": [
                    {
                        "target": "my-third-gcp-project",
                        "assignee": "user:me@myself.com",
                        "action": "roles/owner"
                    }
                ]
            }
        }
    ]
}
