{
    "projectId": "company-d-gew1-project-b",
    "triggers": [
        {
            "triggerTemplate": {
                "repoName": "github_my-github_company-d-gew1-project-b-repo",
                "branchName": "master"
            },
            "filename": "cloudbuild.yaml"
        },
        {
            "triggerTemplate": {
                "repoName": "github_otherguy_some-3rd-party-repo",
                "branchName": "master"
            },
            "runTrigger": {
                "repoName": "github_my-github_company-d-gew1-project-b-repo",
                "branchName": "master"
            }
        }
    ],
    "firewallRules": [
        {
            "name": "my-firewall-rule",
            "direction": "INGRESS/EGRESS",
            "action": "allow/deny",
            "protocolRules": [
                "protocol:port"
            ],
            "description": "my-description",
            "network": "my-network",
            "priority": 1000,
            "sourceRanges": [
                "0.0.0.0/0"
            ]
        }
    ],
    "services": [
        "cloudbuild.googleapis.com",
        "pubsub.googleapis.com",
        "cloudfunctions.googleapis.com"
    ],
    "keyrings": [
        {
            "name": "my-keyring",
            "region": "europe-west1",
            "keys": [
                {
                    "name": "my-key",
                    "purpose": "ENCRYPT_DECRYPT"
                }
            ]
        }
    ],
    "odrlPolicy": {
        "uid": "my-first-gcp-project-policy",
        "permission": [
            {
                "target": "my-keyring",
                "assignee": "user:me@myself.com",
                "action": "roles/cloudkms.cryptoKeyEncrypter"
            },
            {
                "target": "my-keyring",
                "assignee": "serviceAccount:company-d-gew1-project-b@appspot.gserviceaccount.com",
                "action": "roles/cloudkms.cryptoKeyDecrypter"
            },
            {
                "target": "company-d-gew1-project-b",
                "assignee": "user:me@myself.com",
                "action": "roles/owner"
            }
        ]
    }
}
